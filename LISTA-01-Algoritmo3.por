programa {
  cadeia nome
  inteiro idade
  real altura

  funcao inicio() {
    escreva ("\nPor favor, digite o seu nome:\t")
    leia (nome)
    escreva ("\nPor favor, digite a sua idade:\t")
    leia (idade)
    escreva ("\nPor fim, por favor, digite a sua altura:\t")
    leia (altura)

    escreva ("\nOlá\t", nome,". Você possui\t", idade,"\tanos de idade e\t", altura,"m de altura.")
  }
}
